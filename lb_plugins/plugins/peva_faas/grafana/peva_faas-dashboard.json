{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "100 * (1 - avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\", instance=~\"$host\"}[$__rate_interval])))",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Node CPU Usage (%)",
      "type": "timeseries"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "node_memory_MemTotal_bytes{instance=~\"$host\"} - node_memory_MemAvailable_bytes{instance=~\"$host\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Node RAM Usage (bytes)",
      "type": "timeseries"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "100 * (1 - ((node_memory_MemFree_bytes{instance=~\"$host\"} + node_memory_Cached_bytes{instance=~\"$host\"} + node_memory_Buffers_bytes{instance=~\"$host\"}) / node_memory_MemTotal_bytes{instance=~\"$host\"}))",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Node RAM Usage (%)",
      "type": "timeseries"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "unit": "watt"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "id": 4,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "avg_over_time(scaph_host_power_microwatts{instance=~\"$host\"}[$__interval]) / 1000000",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Node Power (W)",
      "type": "timeseries"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "id": 5,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "100 * avg by (function) (avg_over_time(peva_faas_success_rate{run_id=~\"$run_id\", function=~\"$function\"}[$__interval]))",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ],
      "title": "Request Success Rate (%)",
      "type": "timeseries"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "id": 6,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "sum by (function) (rate(peva_faas_request_count{run_id=~\"$run_id\", function=~\"$function\"}[$__rate_interval]))",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ],
      "title": "Throughput by Function (req/s)",
      "type": "timeseries"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 9,
        "w": 16,
        "x": 0,
        "y": 16
      },
      "id": 7,
      "options": {
        "color": {
          "mode": "spectrum"
        },
        "legend": {
          "show": true
        },
        "reverseYBuckets": false,
        "showHistogram": false
      },
      "targets": [
        {
          "expr": "sum by (le, function) (rate(peva_faas_latency_bucket{run_id=~\"$run_id\", function=~\"$function\"}[$__rate_interval]))",
          "format": "heatmap",
          "legendFormat": "{{function}} {{le}}",
          "refId": "A"
        }
      ],
      "title": "Latency Heatmap by Function",
      "type": "heatmap"
    },
    {
      "datasource": "$datasource",
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "OK"
                },
                "1": {
                  "text": "Overload"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 9,
        "w": 8,
        "x": 16,
        "y": 16
      },
      "id": 8,
      "options": {
        "legend": {
          "showLegend": true
        },
        "mergeValues": true,
        "showValue": "never"
      },
      "targets": [
        {
          "expr": "avg by (function) (avg_over_time(peva_faas_success_rate{run_id=~\"$run_id\", function=~\"$function\"}[$__interval])) < bool 0.90",
          "legendFormat": "{{function}}",
          "refId": "A"
        }
      ],
      "title": "Overload Events Timeline",
      "type": "state-timeline"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": [
    "peva_faas"
  ],
